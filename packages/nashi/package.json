{
  "name": "nashi",
  "description": "Nashi means \"nothing\". An almost nothing framework.",
  "version": "0.0.2",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build:esbuild": "esbuild --bundle --format=esm --minify --outfile=dist/index.js src/index.ts",
    "build": "rimraf dist && yarn build:esbuild && tsc --emitDeclarationOnly --declaration",
    "postbuild": "publint",
    "lint": "eslint src/**.ts",
    "lint:fix": "eslint src/**.ts --fix",
    "prerelease": "yarn build",
    "release": "np"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "author": "Yusuke Wada <yusuke@kamawada.com> (https://github.com/yusukebe)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yusukebe/nashi.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "homepage": "https://github.com/yusukebe/nashi",
  "dependencies": {
    "hono": "^3.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.5.0",
    "np": "^8.0.2",
    "publint": "^0.1.12",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}