{
  "name": "sonik",
  "description": "Supersonik web framework for the Edges.",
  "version": "0.0.57",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "vitest --run test/unit",
    "test:integration": "run-s 'test:integration:*'",
    "test:integration:hono-jsx": "yarn vitest run -c ./test/hono-jsx/vitest.config.ts ./test/hono-jsx/integration.test.ts",
    "test:integration:api": "yarn vitest run -c ./test/api/vitest.config.ts ./test/api/integration.test.ts",
    "test:integration:react": "yarn vitest run -c ./test/react/vitest.config.ts ./test/react/integration.test.ts",
    "test:integration:preact": "yarn vitest run -c ./test/preact/vitest.config.ts ./test/preact/integration.test.ts",
    "test:integration:solid": "yarn vitest run -c ./test/solid/vitest.config.ts ./test/solid/integration.test.ts",
    "test:e2e": "run-s 'test:e2e:*'",
    "test:e2e:preact": "yarn playwright test -c test/preact/playwright.config.ts test/preact/e2e.test.ts",
    "test:e2e:react": "yarn playwright test -c test/react/playwright.config.ts test/react/e2e.test.ts",
    "test:all": "yarn test && yarn test:e2e",
    "build": "rimraf dist && tsup && publint",
    "watch": "tsup --watch",
    "lint": "eslint src/**.ts",
    "lint:fix": "eslint src/**.ts --fix",
    "release": "yarn build && yarn test:all && cp ../../README.md ./ && release-it && rm ./README.md"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.js"
    },
    "./react": {
      "types": "./dist/presets/react/index.d.ts",
      "import": "./dist/presets/react/index.js"
    },
    "./react/client": {
      "types": "./dist/presets/react/client.d.ts",
      "import": "./dist/presets/react/client.js"
    },
    "./react/sonikify": {
      "types": "./dist/presets/react/sonikify.d.ts",
      "import": "./dist/presets/react/sonikify.js"
    },
    "./preact": {
      "types": "./dist/presets/preact/index.d.ts",
      "import": "./dist/presets/preact/index.js"
    },
    "./preact/client": {
      "types": "./dist/presets/preact/client.d.ts",
      "import": "./dist/presets/preact/client.js"
    },
    "./solid": {
      "types": "./dist/presets/solid/index.d.ts",
      "import": "./dist/presets/solid/index.js"
    },
    "./solid/client": {
      "types": "./dist/presets/solid/client.d.ts",
      "import": "./dist/presets/solid/client.js"
    }
  },
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client"
      ],
      "vite": [
        "./dist/vite"
      ],
      "react": [
        "./dist/presets/react"
      ],
      "react/client": [
        "./dist/presets/react/client"
      ],
      "react/sonikify": [
        "./dist/presets/react/sonikify"
      ],
      "preact": [
        "./dist/presets/preact"
      ],
      "preact/client": [
        "./dist/presets/preact/client"
      ],
      "solid": [
        "./dist/presets/solid"
      ],
      "solid/client": [
        "./dist/presets/solid/client"
      ]
    }
  },
  "author": "Yusuke Wada <yusuke@kamawada.com> (https://github.com/yusukebe)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yusukebe/sonik.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "homepage": "https://github.com/yusukebe/sonik",
  "dependencies": {
    "@babel/generator": "^7.22.9",
    "@babel/parser": "^7.22.7",
    "@babel/traverse": "^7.22.8",
    "@babel/types": "^7.22.5",
    "@hono/node-server": "^1.1.1"
  },
  "peerDependencies": {
    "hono": "3.x"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@playwright/test": "^1.37.0",
    "@preact/preset-vite": "^2.5.0",
    "@types/babel__core": "^7.20.1",
    "@types/glob": "^8.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "hono": "^3.4.3",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "preact": "^10.16.0",
    "preact-render-to-string": "^6.2.0",
    "publint": "^0.1.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^16.1.3",
    "rimraf": "^5.0.1",
    "solid-js": "^1.7.11",
    "tsup": "^7.2.0",
    "typescript": "5.0",
    "vite": "^4.4.9",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.31.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}